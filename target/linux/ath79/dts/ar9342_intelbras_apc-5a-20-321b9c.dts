#include "ar9344.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "intelbras,apc-5a-20-321b9c", "qca,ar9342";
	model = "Intelbras APC-5A-20-321B9C";

	aliases {
		led-boot = &power_led;
		led-failsafe = &power_led;
		led-running = &power_led;
		led-upgrade = &power_led;
	};

	leds {
		compatible = "gpio-leds";
		
		power_led: power {
			label = "green:power";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
			default-state = "on";
		};

		lan {
			label = "green:lan";
			gpios = <&gpio 4 GPIO_ACTIVE_LOW>;
		};

		rssi0 {
			label = "yellow:rssi0";
			gpios = <&gpio 3 GPIO_ACTIVE_LOW>;
		};

		rssi1{
			label = "yellow:rssi1";
			gpios = <&gpio 2 GPIO_ACTIVE_LOW>;
		};

		rssi2{
			label = "yellow:rssi2";
			gpios = <&gpio 1 GPIO_ACTIVE_LOW>;
		};

		rssi3{
			label = "yellow:rssi3";
			gpios = <&gpio 0 GPIO_ACTIVE_LOW>;
		};

	};
};

&spi {
	status = "okay";
    
	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x000000 0x030000>;
				read-only;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x030000 0x010000>;
				read-only;
			};

			partition@40000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x040000 0xf00000>;
			};

            partition@f40000 {
				label = "data";
				reg = <0xf40000 0x090000>;
				read-only;
			};

			partition@fd0000 {
				label = "cfg";
				reg = <0xfd0000 0x020000>;
				read-only;
			};

            art: partition@ff0000 {
				label = "art";
				reg = <0xff0000 0x010000>;
				read-only;
			};
		};
	};
};

&ref {
	clock-frequency = <40000000>;
};

&mdio0 {
	status = "okay";

	phy0: ethernet-phy@0 {
		phy-mode = "mii";
		reg = <0>;

		qca,ar8327-initvals = <
			0x04 0x07600000 /* PORT0 PAD MODE CTRL */
			0x7c 0x0000007e /* PORT0_STATUS */
		>;
	};
};

&eth0 {
	status = "okay";

	nvmem-cells = <&macaddr_art_0>;
	nvmem-cell-names = "mac-address";
	phy-handle = <&phy0>;
};

&wmac {
	status = "okay";

	ieee80211-freq-limit = <4900000 5990000>;

	nvmem-cells = <&macaddr_art_1002>, <&cal_art_1000>;
	nvmem-cell-names = "mac-address-ascii", "calibration";
};

&art {
	compatible = "nvmem-cells";
	#address-cells = <1>;
	#size-cells = <1>;

	macaddr_art_0: macaddr@0 {
		reg = <0x0 0x6>;
	};

	cal_art_1000: cal@1000 {
		reg = <0x1000 0x440>;
	};

	macaddr_art_1002: macaddr@1002 {
		reg = <0x1002 0x6>;
	};

	cal_art_8000: cal@8000 {
		reg = <0x8000 0x188>;
	};
};
