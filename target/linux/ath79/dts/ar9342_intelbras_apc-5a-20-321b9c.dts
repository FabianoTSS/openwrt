#include "ar9344.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "intelbras,apc-5a-20-321b9c", "qca,ar9342";
	model = "Intelbras APC-5A-20-321B9C";
};

&spi {
	status = "okay";
    
	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x000000 0x030000>;
				read-only;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x030000 0x010000>;
				read-only;
			};

			partition@40000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x040000 0xf00000>;
			};

            partition@f40000 {
				label = "data";
				reg = <0xf40000 0x090000>;
				read-only;
			};

			partition@fd0000 {
				label = "cfg";
				reg = <0xfd0000 0x020000>;
				read-only;
			};

            partition@ff0000 {
				label = "art";
				reg = <0xff0000 0x010000>;
				read-only;
			};
		};
	};
};
